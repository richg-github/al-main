{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "S3RetrieveObjects",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:ListBucketMultipartUploads",
        "s3:GetBucketTagging",
        "s3:GetBucketLogging",
        "s3:ListBucketVersions",
        "s3:GetObjectTagging",
        "s3:ListBucket",
        "s3:GetBucketAcl",
        "s3:GetBucketNotification",
        "s3:GetBucketLocation",
        "s3:ListMultipartUploadParts"
       ],
       "Resource": "arn:aws:s3:::*"
    },
    {
      "Sid": "S3ListBuckets",
      "Effect": "Allow",
      "Action": [
        "s3:ListAllMyBuckets"
       ],
       "Resource": "*"
    }
  ]
}